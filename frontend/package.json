{
  "name": "@group-chat-ai/frontend",
  "type": "module",
  "version": "1.0.0",
  "description": "Frontend React application for Group Chat AI",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node scripts/build-locales.cjs",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "lint:check": "eslint src --ext ts,tsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "typecheck": "tsc --noEmit",
    "i18n:add-language": "node scripts/interactive-add-language.cjs",
    "i18n:add-language-manual": "node scripts/add-language-ai.cjs",
    "i18n:add-language-template": "node scripts/add-language-ai.cjs --template-only",
    "i18n:update-languages": "node scripts/update-languages.cjs",
    "i18n:update-changed": "node scripts/update-changed-translations.cjs",
    "i18n:update-changed-optimized": "node scripts/optimized-update-changed-translations.cjs",
    "build:locales": "node scripts/build-locales.cjs",
    "build:locales:watch": "node scripts/build-locales.cjs --watch",
    "semgrep": "cd .. && source .venv/bin/activate && semgrep --config=semgrep.yml frontend/src/",
    "semgrep:fix": "cd .. && source .venv/bin/activate && semgrep --config=semgrep.yml --autofix frontend/src/",
    "semgrep:json": "cd .. && source .venv/bin/activate && semgrep --config=semgrep.yml --json frontend/src/"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "3.876.0",
    "@cloudscape-design/components": "^3.0.1081",
    "@cloudscape-design/design-tokens": "3.0.0",
    "@cloudscape-design/global-styles": "1.0.0",
    "@group-chat-ai/shared": "1.0.0",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-react": "^5.0.2",
    "axios": "^1.11.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "i18next": "25.4.2",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-http-backend": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "15.7.3",
    "react-oidc-context": "3.1.0",
    "react-router-dom": "6.8.0",
    "uuid": "9.0.0",
    "vite": "6"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@testing-library/jest-dom": "5.16.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.0",
    "@types/glob": "^8.1.0",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.0.0",
    "@types/uuid": "9.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.0.0",
    "jiti": "^2.5.1",
    "typescript-eslint": "^8.42.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
